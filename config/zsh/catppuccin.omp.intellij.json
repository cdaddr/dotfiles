{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "pink": "#F5BDE6",
        "maroon": "#eba0ac",
        "red": "#F38BA8",
        "lavender": "#B7BDF8",
        "blue":  "#8AADF4",
        "sapphire": "#74c7ec",
        "sky": "#99d1db",
        "wight": "#FFFFFF",
        "green": "#a6d189",
        "mauve": "#cba6f7",
        "peach": "#fab387",
        "yellow": "#f9e2af",
        "overlay1": "#7f849c",
        "overlay2": "#9399b2",
        "subtext1": "#bac2de",
        "mantle": "#181825",
        "text": "#11111b"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "transparent",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "template": "\ue0bb",
          "type": "text",
          "force": true
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0bc",
          "leading_diamond": "\ue0ba",
          "style": "diamond",
          "template": "{{.Icon}}",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0bc",
          "style": "diamond",
          "template": "{{ if .SSHSession }} (ssh){{ end }} {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0bc",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "â€¦",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "powerline_symbol": "\ue0bc",
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:red",
          "foreground": "transparent",
          "trailing_diamond": "\ue0bc",
          "style": "diamond",
          "template": "\ue0bb\ue0bb",
          "type": "text",
          "force": true
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "transparent",
          "style": "plain",
          "template": "{{ if ne .Code 0 }}<p:red>({{ .Code }}) </>{{ end }}<p:blue>\uf054</><p:sapphire>\uf054</><p:maroon>\uf054</><p:pink>\uf054</>",
          "type": "text",
          "force": true
        }
      ],
      "alignment": "left",
      "type": "prompt",
      "newline": true
    }
  ],
  "final_space": true,
  "version": 3,
  "async": true
}
